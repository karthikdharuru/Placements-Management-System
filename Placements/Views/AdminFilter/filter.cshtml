@model Placements_Model.Models.Student
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>filterstudent</title>

    <link href="@Url.Content("~/Content/Index.css")" rel="stylesheet" type="text/css" />
    <h2 class="text-danger text-center page-heading">Filter Students</h2>
    <script src="~/Scripts/Placements.js" type="text/javascript"></script>
</head>
<body>
    <div class="container">
        <center>
            <div class="d-flex   justify-content-lg-between justify-content-center flex-wrap" >
                 <div class="filter-option ">
                        <a href="/adminfilter/filterpercentage">
                            <img src="~/Content/images/students.png" class="filter-img" />
                        </a>
                        <p class="text-warning h4">Filter By Percentage</p>
                   </div>

                
                    <div class="filter-option">
                        <a href="/adminfilter/filtercgpa">
                            <img src="~/Content/images/students.png" class="filter-img" />
                        </a>
                        <p class="text-warning h4">Filter By Cgpa</p>
                    </div>
                
               
                    <div class="filter-option">
                        <a href="/adminfilter/filtercompany">
                            <img src="~/Content/images/students.png" class="filter-img" />
                        </a>
                        <p class="text-warning h4">Filter By Company</p>
                    </div>
            </div>
        </center>
    </div>

</body>
</html>